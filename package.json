{
  "name": "tippiq-oss-hood",
  "description": "Tippiq (neighbor)hood",
  "author": "Tippiq <info@tippiq.nl> (http://github.com/alliander)",
  "license": "GPL-3.0-only",
  "version": "4.32.1",
  "engines": {
    "node": "6.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alliander/tippiq-oss-hood.git"
  },
  "homepage": "https://hood.tippiq.nl",
  "keywords": [
    "tippiq",
    "hood"
  ],
  "private": true,
  "scripts": {
    "test:start": "DEBUG=tippiq-hood:* npm-run-all -ln prestart seed-run start & npm-run-all -ln wait-for-frontend",
    "prestart": "npm-run-all -ln wait-for-db migrate-latest",
    "start": "NODE_ENV=production npm-run-all run:prod:servers",
    "dev": "NODE_ENV=development npm-run-all dev:build",
    "dev:build": "npm-run-all -ln clear:dist prepare:dev:build development",
    "development": "npm-run-all -ln --parallel build:dev:client build:dev:server run:dev:servers",
    "prepare:dev:build": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "build:dev:client": "better-npm-run run_dev_client",
    "build:dev:server": "better-npm-run build_dev_server",
    "run:dev:servers": "npm-run-all --parallel delay run:dev:api:server delay run:dev:server",
    "run:dev:server": "better-npm-run run_dev_server",
    "run:dev:api:server": "nodemon --watch api --exec babel-node api/server.js",
    "clear:dist": "rm -rf static/dist",
    "clear:dependencies": "rm -rf node_modules",
    "build": "npm-run-all clear:dist prod:build:client delay prod:build:server",
    "prod": "npm-run-all prod:build:client delay prod:build:server run:prod:servers",
    "prod:build:client": "better-npm-run build_prod_client",
    "prod:build:server": "better-npm-run build_prod_server",
    "run:prod:servers": "npm-run-all --parallel run:prod:api:server delay run:prod:server",
    "run:prod:server": "better-npm-run run_prod_server",
    "run:prod:api:server": "babel-node api/server.js",
    "delay": "node ./sleep 1000",
    "jsdoc": "npm-run-all 'jsdoc:*'",
    "jsdoc:src": "jsdoc -c jsdoc.src.json; : 'ignore minor errors'",
    "jsdoc:api": "jsdoc -c jsdoc.api.json; : 'ignore minor errors'",
    "lint": "eslint -c .eslintrc api src",
    "lint:fix": "eslint -c .eslintrc api src --fix",
    "watch:test:unit": "better-npm-run watch_test_unit",
    "test:src:unit": "better-npm-run test_unit",
    "pretest:src:e2e": "npm-run-all webdriver-update wait-for-frontend",
    "test:src:e2e": "protractor ./protractor.conf.js",
    "test:api:e2e": "mocha ./mocha-init.js 'api/**/*.e2e.js'",
    "test:api:unit": "mocha 'api/**/*.spec.js'",
    "migrate-latest": "knex migrate:latest",
    "seed-run": "knex seed:run",
    "wait-for-frontend": "wget --retry-connrefused --spider ${PROTRACTOR_URL:-http://localhost:3007}",
    "wait-for-db": "wait-for-db",
    "webdriver-update": "webdriver-manager update",
    "send-weekly-newsletter": "babel-node jobs/send-weekly-newsletter",
    "docs": "raml2html docs/api/index.raml > src/static/docs/index.html"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test))\\.(js|jsx)$",
    "scriptPreprocessor": "./jest-preprocessor.js",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "globals": {
      "__CLIENT__": true
    }
  },
  "betterScripts": {
    "run_dev_client": {
      "command": "babel-node webpack/server.dev.js",
      "env": {
        "PORT": 3006
      }
    },
    "build_dev_server": {
      "command": "webpack --config ./webpack/webpack.config.server.dev.entry.js --watch --hot --inline --colors --display-error-details"
    },
    "run_dev_server": {
      "command": "nodemon --watch ./src/server.js --watch ./src --watch ./static/dist/server.js ./src/start-server.js --exec babel-node",
      "env": {
        "PORT": 3007,
        "DEBUG": "tippiq-hood:*",
        "DEBUG_LEVELS": "trace"
      }
    },
    "build_prod_client": {
      "command": "webpack --colors --display-error-details --config ./webpack/webpack.config.client.prod.entry.js"
    },
    "build_prod_server": {
      "command": "webpack --colors --display-error-details --config ./webpack/webpack.config.server.prod.entry.js"
    },
    "run_prod_server": {
      "command": "./node_modules/.bin/babel-node ./src/start-server-prod.js",
      "env": {
        "PORT": 3007
      }
    },
    "test_unit": "jest --coverage --no-cache",
    "watch_test_unit": "jest --coverage --watch --no-cache"
  },
